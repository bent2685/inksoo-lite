{
  "name": "inksoo-lite",
  "version": "0.0.1 beta",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "bent2685",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev --inspect --sourcemap",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@milkdown/core": "^7.3.3",
    "@milkdown/ctx": "^7.3.3",
    "@milkdown/plugin-history": "^7.3.3",
    "@milkdown/plugin-prism": "^7.3.3",
    "@milkdown/preset-commonmark": "^7.3.3",
    "@milkdown/preset-gfm": "^7.3.3",
    "@milkdown/prose": "^7.3.3",
    "@milkdown/transformer": "^7.3.3",
    "@milkdown/vue": "^7.3.3",
    "@prosemirror-adapter/vue": "^0.2.6",
    "@tailwindcss/typography": "^0.5.10",
    "@vueuse/core": "^10.7.2",
    "electron-acrylic-window": "^0.5.11",
    "electron-updater": "^6.1.7",
    "pinia": "^2.1.7",
    "remark-directive": "^3.0.0",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@iconify-json/tabler": "^1.1.105",
    "@milkdown/plugin-listener": "^7.3.5",
    "@rushstack/eslint-patch": "^1.7.1",
    "@types/node": "^18.19.9",
    "@unocss/preset-attributify": "^0.58.4",
    "@unocss/preset-icons": "^0.58.4",
    "@unocss/preset-uno": "^0.58.4",
    "@unocss/transformer-directives": "^0.58.4",
    "@unocss/transformer-variant-group": "^0.58.4",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "lodash": "^4.17.21",
    "prettier": "^3.2.4",
    "sass": "^1.70.0",
    "typescript": "^5.3.3",
    "unocss": "^0.58.4",
    "vite": "^5.0.12",
    "vue": "^3.4.15",
    "vue-tsc": "^1.8.27"
  }
}
